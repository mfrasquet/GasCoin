#En una ventana creamos el nodo:
geth --rpc --rpccorsdomain "*" --rpcport 8545 --rpcapi eth,web3,personal --datadir ~/rinkeby --rinkeby --syncmode light

#En otra ventana hacemos el attach de la consola geth
geth attach ~/rinkeby/geth.ipc 

#Inicializo cuentas y direcciones
me=eth.accounts[0] 
GS1=eth.accounts[1]
